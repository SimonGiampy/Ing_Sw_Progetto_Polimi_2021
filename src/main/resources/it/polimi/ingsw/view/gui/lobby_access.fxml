<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1"
            stylesheets="@../../../../../assets/bootstrap3.css" fx:controller="it.polimi.ingsw.view.gui.LobbyAccess">
    <children>
        <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../../assets/lobby_access_uffizi.jpg" />
            </image>
        </ImageView>
        <ListView fx:id="lobbyList" layoutX="200.0" layoutY="230.0" prefHeight="700.0" prefWidth="400.0" />
        <Button fx:id="btnCreateLobby" layoutX="1250.0" layoutY="230.0" mnemonicParsing="false" prefHeight="193.0" prefWidth="290.0" text="Create Lobby">
            <styleClass>
                <String fx:value="info" />
                <String fx:value="lg" />
            </styleClass>
        </Button>
        <Button fx:id="btnJoinLobby" layoutX="1250.0" layoutY="731.0" mnemonicParsing="false" prefHeight="193.0" prefWidth="290.0" text="Join Lobby">
            <styleClass>
                <String fx:value="info" />
                <String fx:value="lg" />
            </styleClass>
        </Button>
    </children>
</AnchorPane>
